{
  "functions": {
    "source": "backend/",
    "function": "api",
    "runtime": "nodejs16"
  },
  "hosting": {
    "public": "frontend/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "/api/**",
      "function": "api"
    }, {
      "source": "**",
      "destination": "/index.html"
    }]
  }
}